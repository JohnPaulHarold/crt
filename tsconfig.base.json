{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"allowJs": true,
		"checkJs": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"composite": true,
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true
	}
}
